<div >
     <h3>Add new item &nbsp<a href="/shop/create" class="btn  btn-primary" role="button">Add</a>
  &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp &nbsp  <a href="/" class="btn btn-primary pull-right" role="button">Logout</a> 
      
 </div>
{{# each products}}
<div class="row">
  {{# each this}}
<div class="col-sm-6 col-md-4">

   <div class="thumbnail">
     <img src={{this.imagePath}} alt=".." class="img-responsive">
   <div class="caption">
     <h3>{{this.title}}</h3>
     <p class="description"> {{this.description}}</p>
     <div class="clearfix">
  <div class="price pull-left">RS.  {{this.price}}</div>
  <a href="/delete/{{this._id}}" class="btn btn-primary pull-right" role="button">Delete</a>
        </div>
      </div>
     </div>
   </div>
   {{/each}}
  </div>
{{/each}}
 